    private void txtCariKeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtCariKeyPressed
        try {
            // TODO add your handling code here:
            recBuku = daoBuku.getById(txtCari.getText());
            isiTabelCari();
        } catch (SQLException ex) {
            Logger.getLogger(TransaksiPasok.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_txtCariKeyPressed

    private void btnHapusActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnHapusActionPerformed
        // TODO add your handling code here:
        recPasok.remove(tabelPasok.getSelectedRow());
        this.isiTabelPasok();
    }//GEN-LAST:event_btnHapusActionPerformed

    private void txtProsesActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_txtProsesActionPerformed
        // TODO add your handling code here:
        for (Pasok p : recPasok) {
            try {
                daoPasok.insert(p);
                System.out.println(p.getJumlah());
            } catch (SQLException ex) {
                Logger.getLogger(TransaksiPasok.class.getName()).log(Level.SEVERE, null, ex);
            }
        }
        recBuku.clear();
        JOptionPane.showMessageDialog(this, "Proses Transaksi Pasok Buku Berhasil..", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        txtID.setText("");
        this.statusAwal();
    }//GEN-LAST:event_txtProsesActionPerformed

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        try {
            // TODO add your handling code here:
            Buku b = daoBuku.getByOne(txtID.getText());
            Pasok p = new Pasok();
            Distributor d = daoDistributor.getByNama((String) cmbDistributor.getSelectedItem());
            p.setDistributor(d);
            p.setBuku(b);
            p.setJumlah(Integer.parseInt(txtJumlah.getText()));
            p.setTanggal(txtTanggal.getDate());
            recPasok.add(p);
            txtCari.setText("");
            txtJumlah.setText("");
            txtID.setText("");
            this.isiTabelPasok();



        } catch (SQLException ex) {
            Logger.getLogger(TransaksiPasok.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_jButton1ActionPerformed

    private void btnOk1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnOk1ActionPerformed
        // TODO add your handling code here:
        Buku b = recBuku.get(tabelCari.getSelectedRow());
        txtID.setText(b.getId());
        txtJumlah.requestFocus();
        jDialog1.setVisible(false);
    }//GEN-LAST:event_btnOk1ActionPerformed
