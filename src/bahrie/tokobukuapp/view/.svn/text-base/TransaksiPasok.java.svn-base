    void loadCombo() {
        cmbDistributor.removeAllItems();
        try {
            recDistributor = daoDistributor.getAll();
            for (Distributor d : recDistributor) {
                cmbDistributor.addItem(d.getNama());
            }
        } catch (SQLException ex) {
            Logger.getLogger(TransaksiPasok.class.getName()).log(Level.SEVERE, null, ex);
        }
    }

    void kosongkanTabel() {
        tabelPasok.setModel(new DefaultTableModel(new Object[][]{{null, null, null}}, new String[]{"ID Buku", "Judul", "Jumlah"}));
    }

    void isiTabelPasok() {
        Object data[][] = new Object[recPasok.size()][3];
        int x = 0;
        for (Pasok p : recPasok) {
            data[x][0] = p.getBuku().getId();
            data[x][1] = p.getBuku().getJudul();
            data[x][2] = p.getJumlah();
            x++;
        }
        String judul[] = {"IDBuku", "Judul", "Jumlah"};
        tabelPasok.setModel(new DefaultTableModel(data, judul));
        jScrollPane1.setViewportView(tabelPasok);
    }

    void isiTabelCari() {
        Object data[][] = new Object[recBuku.size()][3];
        int x = 0;
        for (Buku b : recBuku) {
            data[x][0] = b.getId();
            data[x][1] = b.getJudul();
            data[x][2] = b.getHargaPokok();
            x++;
        }
        String judul[] = {"ID", "Judul", "Harga"};
        tabelCari.setModel(new DefaultTableModel(data, judul));
        jScrollPane2.setViewportView(tabelCari);
    }

    void setTanggal() {
        Date sekarang = Calendar.getInstance().getTime();
        txtTanggal.setDate(sekarang);
    }

    void statusAwal() {
        loadCombo();
        setTanggal();
        kosongkanTabel();
    }

